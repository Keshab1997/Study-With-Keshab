{
  "name": "Study With Keshab",
  "short_name": "SWK",
  "description": "Your ultimate educational partner. Learn, practice, and excel with high-quality courses and notes from Keshab.",
  "lang": "bn-BD",
  "dir": "ltr",
  "start_url": ".",
  "scope": "/Study-With-Keshab/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "minimal-ui"],
  "background_color": "#f0f2f5",
  "theme_color": "#4a90e2",
  "orientation": "portrait-primary",
  "categories": ["education", "books", "personal-growth"],
  "icons": [
    { "src": "/Study-With-Keshab/images/icons/icon-72x72.png", "sizes": "72x72", "type": "image/png" },
    { "src": "/Study-With-Keshab/images/icons/icon-96x96.png", "sizes": "96x96", "type": "image/png" },
    { "src": "/Study-With-Keshab/images/icons/icon-128x128.png", "sizes": "128x128", "type": "image/png" },
    { "src": "/Study-With-Keshab/images/icons/icon-144x144.png", "sizes": "144x144", "type": "image/png" },
    { "src": "/Study-With-Keshab/images/icons/icon-152x152.png", "sizes": "152x152", "type": "image/png" },
    { "src": "/Study-With-Keshab/images/icons/icon-192x192.png", "sizes": "192x192", "type": "image/png", "purpose": "any" },
    { "src": "/Study-With-Keshab/images/icons/icon-384x384.png", "sizes": "384x384", "type": "image/png" },
    { "src": "/Study-With-Keshab/images/icons/icon-512x512.png", "sizes": "512x512", "type": "image/png", "purpose": "any" },
    { "src": "/Study-With-Keshab/images/icons/icon-maskable-512x512.png", "sizes": "512x512", "type": "image/png", "purpose": "maskable" }
  ],
  "screenshots": [
    {
      "src": "/Study-With-Keshab/images/screenshots/mobile_screenshot1.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Home Screen showing available courses"
    },
    {
      "src": "/Study-With-Keshab/images/screenshots/desktop_screenshot1.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Course details view on desktop"
    }
  ],
  "shortcuts": [
    {
      "name": "My Courses",
      "url": "/Study-With-Keshab/courses/",
      "description": "Quick access to your enrolled courses.",
      "icons": [{ "src": "/Study-With-Keshab/images/icons/shortcut-courses-96x96.png", "sizes": "96x96" }]
    },
    {
      "name": "Daily Quiz",
      "url": "/Study-With-Keshab/quiz/",
      "description": "Take the daily quiz to test your knowledge.",
      "icons": [{ "src": "/Study-With-Keshab/images/icons/shortcut-quiz-96x96.png", "sizes": "96x96" }]
    }
  ],
  "share_target": {
    "action": "/Study-With-Keshab/share-receiver/",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/Study-With-Keshab/open-pdf",
      "accept": {
        "application/pdf": [".pdf"]
      },
      "icons": [
        { "src": "/Study-With-Keshab/images/icons/file-handler-pdf.png", "sizes": "128x128" }
      ]
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+swk",
      "url": "/Study-With-Keshab/protocol?data=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.example.swk",
      "id": "com.example.swk"
    }
  ]
}